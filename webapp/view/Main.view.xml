<mvc:View xmlns:widgets="sap.ui.integration.widgets" xmlns:filterbar="sap.ui.comp.filterbar"
	controllerName="Thesis.thesis.controller.Main"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>

<Page id="MainPage">

				<FlexBox
				height="100px"
				alignItems="Start"
				justifyContent="End">
					<Button 
					width="100%"
					text="Kilépés"
					press="onExit"
			></Button>
				</FlexBox>
						<SearchField width="50%" placeholder="Keresés" search="onSearch" />
			
				<filterbar:FilterBar >
				</filterbar:FilterBar>
</Page>
</mvc:View>